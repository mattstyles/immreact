{
  "name": "immreact",
  "version": "0.0.1",
  "description": "Use react classes with an immstruct structure",
  "main": "lib/",
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "preexample": "npm run build",
    "example": "browserify -t babelify --debug example/basic.js > example/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "immstruct",
    "immutable",
    "state",
    "centralised",
    "es6",
    "es2015",
    "babel"
  ],
  "author": "Matt Styles",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "react-dom": "^0.14.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "immstruct": "^2.0.0",
    "immutable": "^3.7.5"
  },
  "dependencies": {
    "eventemitter3": "^1.1.1"
  }
}
