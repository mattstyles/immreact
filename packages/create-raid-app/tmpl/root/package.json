{
  "name": "{{projectName}}",
  "private": true,
  "version": "0.0.0",
  "description": "@TODO description",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_PATH=src NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "NODE_PATH=src DEBUG=true parcel src/index.html"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "styled-components",
      "transform-react-require",
      "lodash",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{username}}/{{projectName}}.git"
  },
  "keywords": [
    "raid",
    "react"
  ],
  "author": "{{username}}",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/{{username}}/{{projectName}}/issues"
  },
  "homepage": "https://{{username}}.github.io/{{projectName}}",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-require": "^1.0.1",
    "create-raid-app": "^4.0.0",
    "gh-pages": "^2.0.1",
    "mkdirp": "^1.0.3",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.2",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "most": "^1.8.0",
    "prop-types": "^15.7.2",
    "raid": "^4.0.0",
    "raid-addons": "^4.0.0",
    "raid-navigator": "^4.0.0",
    "raid-streams": "^4.0.0",
    "react": "^16.12.0",
    "react-basic-kit": "^0.11.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "reselect": "4.0.0",
    "styled-components": "^5.0.1"
  }
}
