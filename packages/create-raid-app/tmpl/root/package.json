{
  "name": "{{projectName}}",
  "private": true,
  "version": "0.0.0",
  "description": "@TODO description",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_PATH=src NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "NODE_PATH=src DEBUG=true parcel src/index.html"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "styled-components",
      "transform-react-require",
      "lodash",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{{username}}/{{projectName}}.git"
  },
  "keywords": [
    "raid",
    "react"
  ],
  "author": "{{username}}",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/{{username}}/{{projectName}}/issues"
  },
  "homepage": "https://{{username}}.github.io/{{projectName}}",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-react-require": "^1.0.1",
    "create-raid-app": "^5.0.0",
    "gh-pages": "^2.2.0",
    "mkdirp": "^1.0.4",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "standard": "^14.3.3"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "most": "^1.8.0",
    "prop-types": "^15.7.2",
    "raid": "^5.0.0",
    "@raid/addons": "^5.0.0",
    "@raid/app-kit": "^0.16.0",
    "@raid/basic-kit": "^0.16.0",
    "@raid/navigator": "^5.0.0",
    "@raid/streams": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-is": "^16.13.1",
    "reselect": "4.0.0",
    "styled-components": "^5.1.0",
    "styled-system": "5.1.5",
    "@styled-system/css": "5.1.5",
    "@styled-system/theme-get": "5.1.2",
    "@styled-system/variant": "5.1.5"
  }
}
