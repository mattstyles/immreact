{
  "name": "@raid/streams",
  "version": "6.0.0",
  "description": "Collection of event streams ready to mount to raid",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir ./",
    "watch": "babel --watch src --out-dir ./",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc --reporter=lcov npm run test",
    "prepublish": "yarn run build",
    "prestart": "yarn run build",
    "start": "yarn run watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "sideEffects": false,
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "keywords": [
    "raid",
    "addons",
    "helpers",
    "utility",
    "streams",
    "events",
    "mount"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "standard": "^14.3.1"
  },
  "dependencies": {
    "most": "^1.8.0",
    "raf-stream": "^3.0.0",
    "vkey": "^1.0.1"
  },
  "gitHead": "133ec598275946f14fe0a0e8ef1fcdae8e2ba3d6"
}
