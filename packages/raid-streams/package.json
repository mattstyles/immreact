{
  "name": "raid-streams",
  "version": "3.3.1",
  "description": "Collection of event streams ready to mount to raid",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir ./",
    "watch": "babel --watch src --out-dir ./",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc --reporter=lcov npm run test",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "start": "npm run watch"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "addons",
    "helpers",
    "utility",
    "streams",
    "events",
    "mount"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.4",
    "nyc": "^14.1.1",
    "raid": "^3.3.1",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "most": "^1.7.3",
    "raf-stream": "^3.0.0",
    "vkey": "^1.0.1"
  }
}
