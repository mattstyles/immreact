{
  "name": "@raid/streams",
  "version": "5.0.0",
  "description": "Collection of event streams ready to mount to raid",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir ./",
    "watch": "babel --watch src --out-dir ./",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc --reporter=lcov npm run test",
    "prepublish": "yarn run build",
    "prestart": "yarn run build",
    "start": "yarn run watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "sideEffects": false,
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "addons",
    "helpers",
    "utility",
    "streams",
    "events",
    "mount"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.0.9",
    "nyc": "^15.0.0",
    "raid": "^5.0.0",
    "rimraf": "^3.0.2",
    "standard": "^14.3.1",
    "tape": "^4.13.0"
  },
  "dependencies": {
    "most": "^1.8.0",
    "raf-stream": "^3.0.0",
    "vkey": "^1.0.1"
  },
  "gitHead": "b4b522859ba4d821738de98caaabc7722fb2f736"
}
