{
  "name": "raid-fl",
  "version": "3.3.1",
  "description": "Raid with fantasy-land compliant structures",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib && rimraf compat",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build:latest": "babel src --out-dir lib",
    "build:compat": "BABEL_ENV=compat babel src --out-dir compat",
    "build": "npm run build:compat && npm run build:latest",
    "watch": "babel --watch src --out-dir lib",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc --reporter=lcov npm run test",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "start": "npm run watch"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Edge versions",
              "last 2 iOS versions",
              "last 2 Firefox versions"
            ]
          }
        }
      ]
    ],
    "env": {
      "compat": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "fantasy-land",
    "helpers",
    "utility"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.4",
    "nyc": "^14.1.1",
    "raid": "^3.3.1",
    "ramda": "^0.26.1",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "fantasy-land": "^3.5.0",
    "force-array": "^3.1.0"
  }
}
