{
  "name": "raid-navigator",
  "version": "0.0.0",
  "description": "Navigation stack hooked in to raid signal",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "yarn run build",
    "start": "yarn run watch",
    "test": "tape -r babel-register spec/*.test.js",
    "cover": "nyc tape -r babel-register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "yarn run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "urban"
    ],
    "plugins": [
      "transform-react-require",
      "styled-jsx/babel"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "navigator",
    "router",
    "history",
    "stack"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-urban": "^1.0.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.14",
    "nyc": "^10.1.2",
    "raid": "*",
    "rimraf": "^2.5.2",
    "standard": "^9.0.1",
    "styled-jsx": "^0.5.7",
    "tape": "^4.6.2"
  },
  "license": "MIT",
  "dependencies": {
    "raid-addons": "^2.3.0"
  }
}
