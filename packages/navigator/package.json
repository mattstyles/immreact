{
  "name": "raid-navigator",
  "version": "4.0.0",
  "description": "Navigation stack hooked in to a raid signal",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "yarn run build",
    "start": "yarn run watch",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-react-require",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "navigator",
    "router",
    "history",
    "stack"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "coveralls": "^3.0.9",
    "jsdom": "^16.2.0",
    "nyc": "^15.0.0",
    "raid": "^4.0.0",
    "rimraf": "^3.0.2",
    "standard": "^14.3.1",
    "tape": "^4.13.0"
  },
  "dependencies": {
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "raid-addons": "^4.0.0",
    "reselect": "^4.0.0"
  },
  "peerDependencies": {
    "raid": "^3.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "license": "MIT",
  "gitHead": "b4b522859ba4d821738de98caaabc7722fb2f736"
}
