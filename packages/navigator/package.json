{
  "name": "raid-navigator",
  "version": "3.0.1",
  "description": "Navigation stack hooked in to a raid signal",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "npm run build",
    "start": "npm run watch",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "npm run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-react-require",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "navigator",
    "router",
    "history",
    "stack"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.4",
    "jsdom": "^15.1.1",
    "nyc": "^14.1.1",
    "raid": "^3.0.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "raid-addons": "^3.0.0",
    "reselect": "^4.0.0"
  },
  "peerDependencies": {
    "raid": "^3.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  },
  "license": "MIT"
}
