{
  "name": "raid-navigator",
  "version": "2.4.0",
  "description": "Navigation stack hooked in to raid signal",
  "main": "lib",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "yarn run clean",
    "build": "BABEL_ENV=react babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "yarn run build",
    "start": "yarn run watch",
    "test": "BABEL_ENV=react tape -r babel-register spec/*.test.js",
    "cover": "BABEL_ENV=react nyc tape -r babel-register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "yarn run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "env": {
      "react": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              },
              "loose": true
            }
          ],
          "stage-3",
          "react"
        ],
        "plugins": [
          "transform-react-require",
          "transform-class-properties",
          "styled-jsx/babel",
          [
            "transform-define",
            {
              "process.env.BABEL_ENV": "./env/react"
            }
          ]
        ]
      },
      "inferno": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              },
              "loose": true
            }
          ],
          "stage-3"
        ],
        "plugins": [
          [
            "transform-react-require",
            {
              "identifier": "Inferno",
              "moduleName": "inferno"
            }
          ],
          "inferno",
          "transform-class-properties",
          "styled-jsx/babel",
          [
            "transform-define",
            {
              "process.env.BABEL_ENV": "./env/react"
            }
          ]
        ]
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "navigator",
    "router",
    "history",
    "stack"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-inferno": "^0.7.7",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-define": "^1.2.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.14",
    "envify": "^4.0.0",
    "jsdom": "^9.12.0",
    "nyc": "^10.1.2",
    "raid": "^2.4.0",
    "rimraf": "^2.5.2",
    "standard": "^9.0.1",
    "styled-jsx": "^0.5.7",
    "tape": "^4.6.2"
  },
  "dependencies": {
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "raid-addons": "^2.4.0",
    "reselect": "^3.0.0"
  },
  "peerDependencies": {
    "inferno": "^0.7.27",
    "inferno-component": "^0.7.27",
    "inferno-dom": "^0.7.27",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "license": "MIT"
}
