{
  "name": "raid",
  "version": "6.0.0",
  "description": "Centralised state container",
  "main": "lib/",
  "scripts": {
    "bench": "node -r @babel/register bench/tape",
    "clean": "rimraf lib",
    "lint": "standard",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "test": "tape -r @babel/register spec/*.test.js",
    "cover": "nyc tape -r @babel/register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "yarn run build",
    "prestart": "yarn run build",
    "start": "yarn run watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  },
  "sideEffects": false,
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "keywords": [
    "raid",
    "stream",
    "scan",
    "state",
    "signal",
    "component"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "pretty-hrtime": "^1.0.3",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@most/hold": "^2.0.0",
    "eventemitter3": "^4.0.0",
    "most": "^1.8.0"
  },
  "gitHead": "133ec598275946f14fe0a0e8ef1fcdae8e2ba3d6"
}
