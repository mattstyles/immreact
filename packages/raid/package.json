{
  "name": "raid",
  "version": "2.1.1",
  "description": "Centralised state container",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "yarn run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "yarn run build",
    "start": "yarn run watch",
    "pretest": "yarn run build",
    "test": "nyc tape -r babel-register spec/*.js",
    "test-only": "tape -r babel-register spec/*.js",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "yarn run build"
  },
  "babel": {
    "presets": [
      "urban"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "stream",
    "scan",
    "state",
    "signal",
    "component"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-urban": "^1.0.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.14",
    "nyc": "^10.1.2",
    "rimraf": "^2.5.4",
    "standard": "^9.0.1",
    "tape": "^4.6.2"
  },
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "most": "^1.0.3"
  }
}
