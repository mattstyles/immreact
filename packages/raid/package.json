{
  "name": "raid",
  "version": "2.7.0",
  "description": "Centralised state container",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "watch": "babel --watch src --out-dir lib",
    "prestart": "npm run build",
    "start": "npm run watch",
    "test": "tape -r babel-register spec/*.test.js",
    "cover": "nyc tape -r babel-register spec/*.test.js",
    "coverage-report": "nyc report --reporter=lcov",
    "prepublish": "npm run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "urban"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "raid",
    "stream",
    "scan",
    "state",
    "signal",
    "component"
  ],
  "author": "Matt Styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstyles/raid"
  },
  "bugs": "https://github.com/mattstyles/raid/issues",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-urban": "^1.0.1",
    "babel-register": "^6.26.0",
    "coveralls": "^2.11.14",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.2",
    "standard": "^9.0.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "eventemitter3": "^2.0.2",
    "most": "^1.7.1"
  }
}
